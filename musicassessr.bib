	year = {2012},
	keywords = {Models, Statistical Analysis, Structural Equation Modeling},
	pages = {247--260},
	file = {Snapshot:/Users/sebsilas/Zotero/storage/YSXB2CM4/2012-16551-015.html:text/html},
}

