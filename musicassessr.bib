	year = {2011},
	pmid = {21409740},
	note = {Number: 4
Reporter: Quarterly Journal of Experimental Psychology (2006)},
	keywords = {Humans, Music, Models, Psychological, Acoustic Stimulation, Auditory Perceptual Disorders, Pitch Discrimination},
	pages = {625--638},
}

@article{science_motivation_2002,
	title = {Motivation and working memory},
