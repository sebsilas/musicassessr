Reporter: Philosophical Studies},
	keywords = {Capacities, Causal inference, Contributions, External validity, Probabilistic theory of causality, Randomized controlled trials (RCTs)},
	pages = {59},
}

@article{mrazik_neurobiological_2010,
	title = {The neurobiological foundations of giftedness},
	volume = {32},
	issn = {0278-3193},
