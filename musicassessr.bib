	author = {Király, Susanna},
	month = jul,
	year = {2016},
	note = {Google-Books-ID: zqS3DAAAQBAJ},
	keywords = {Music / Reference},
}

@article{henry_vocal_2015,
	title = {Vocal sight-reading assessment: technological advances, student perceptions, and instructional implications},
